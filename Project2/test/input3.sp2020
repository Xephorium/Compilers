declare x := 0 ;
{
in x ;
loop [ x + 2 / 3 < 0 - * 3 ]
  iffy [ x < > 0 ] then 
    out x ; ; ;

goto x ;
    iffy [ x < 0 ] then
      iffy [ x < > 0 ] then
  label x ; ; ;

  in x ;
    iffy [ x < 0 ] then
      iffy [ x < > 0 ] then
        out x ; ; ;

loop [ x < 0 ]
  {
    x := 5 ;
    out x ;
  };
}
